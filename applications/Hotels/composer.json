{
    "name": "rezyon/application-hotels",
    "version": "1.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rezyon\\Applications\\Hotels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "rezyon/hotels": "*"
    },
    "extra": {
