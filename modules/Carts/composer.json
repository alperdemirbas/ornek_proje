{
  "name": "rezyon/carts",
  "version": "1.0",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Rezyon\\Carts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "rezyon/discounts": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rezyon\\Carts\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^3.0"
  }
}
